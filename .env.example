# Go High Level Configuration
# Copy this file to .env and fill in your actual values

# Go High Level Webhook URL
# Get this from: Settings → Webhooks → Create New Webhook
GHL_WEBHOOK_URL=https://hooks.gohighlevel.com/your-webhook-id

# Optional: Go High Level API Key (if using direct API instead of webhooks)
GHL_API_KEY=your-api-key-here

# Optional: Location ID (for Go High Level)
GHL_LOCATION_ID=your-location-id

# Optional: Custom Source Tracking
SOURCE_NAME=Pray Music Player

# Optional: Custom Field Mappings
# If you have custom fields in Go High Level, map them here
CUSTOM_FIELD_MUSIC_PREFERENCE=
CUSTOM_FIELD_REFERRAL_SOURCE=
